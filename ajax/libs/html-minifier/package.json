{
  "name": "html-minifier",
  "description": "Highly configurable, well-tested, JavaScript-based HTML minifier.",
  "version": "3.2.3",
  "filename": "htmlminifier.min.js",
  "keywords": [
    "cli",
    "compress",
    "compressor",
    "css",
    "html",
    "htmlmin",
    "javascript",
    "min",
    "minification",
    "minifier",
    "minify",
    "optimize",
    "optimizer",
    "pack",
    "packer",
    "parse",
    "parser",
    "uglifier",
    "uglify"
  ],
  "homepage": "https://kangax.github.io/html-minifier/",
  "author": "Juriy Zaytsev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kangax/html-minifier.git"
  },
  "dependencies": {
    "camel-case": "3.0.x",
    "clean-css": "3.4.x",
    "commander": "2.9.x",
    "he": "1.1.x",
    "ncname": "1.0.x",
    "param-case": "2.1.x",
    "relateurl": "0.2.x",
    "uglify-js": "2.7.x"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/kangax/html-minifier.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
